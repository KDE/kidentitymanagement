#, kde-format
msgid "This text file size exceeds 1kb."
msgstr "O tamanho deste ficheiro de texto ultrapassa 1kb."

#: widgets/signatureconfigurator.cpp:417
#, kde-format
msgid "Text File Size"
msgstr "Tamanho do Ficheiro de Texto"

